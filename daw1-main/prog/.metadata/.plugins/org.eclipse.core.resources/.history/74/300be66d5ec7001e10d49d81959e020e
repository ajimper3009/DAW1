package ejerciciosClase.unidad5.empresa;

public class Empresa extends Empleado {

	//Atributos
	private String nombre;
	private String cif;
	private int telefono;
	private String direccion;
	private String [] empleados;
	
	//Constructores
	
	public Empresa(String nombre, String apellidos, String dni, double sueldoBruto) {
		super(nombre, apellidos, dni, sueldoBruto);
		// TODO Auto-generated constructor stub
		
		this.nombre = nombre;
		this.cif = cif;
		this.telefono = telefono;
		this.direccion = direccion;
		this.empleados = empleados [100];
	}
	
}
